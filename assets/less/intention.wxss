/*变量*/
/*常用的一些混合*/
.ellip {
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.after-cls:after {
  content: " ";
  display: block;
  height: 0;
  visibility: hidden;
  clear: both;
}
.display-flex {
  display: -webkit-box;
  display: -moz-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
}
.forbid {
  opacity: 0.8 !important;
  cursor: not-allowed;
}
.intention-page .intention-banner {
  position: relative;
  width: 100%;
  height: 300rpx;
  overflow: hidden;
}
.intention-page .intention-banner image {
  width: 100%;
  height: 100%;
}
.intention-page .intention-banner .price {
  position: absolute;
  left: 80rpx;
  top: 200rpx;
  font-size: 0;
  font-family: Impact, Haettenschweiler, 'Arial Narrow Bold', sans-serif;
  color: #fff;
  line-height: 30px;
}
.intention-page .intention-banner .price .wx-i {
  position: relative;
  bottom: -4px;
  font-size: 16px;
  margin-right: 2px;
  vertical-align: middle;
}
.intention-page .intention-banner .price .wx-font {
  position: relative;
  font-size: 30px;
  margin-right: 2px;
  vertical-align: middle;
}
.intention-page .intention-banner .price .wx-font .hot {
  position: absolute;
  top: -8px;
  right: -16px;
  width: 18px;
  height: 18px;
  background: url(https://m.waitui.com/htdocs/mobile/images/entry/entry_subject_fire.png) no-repeat;
  background-size: 100% 100%;
}
.intention-page .intention-summary {
  background-color: #fff;
  padding-bottom: 15px;
}
.intention-page .intention-summary .summary-box {
  padding: 10px 15px;
  border-bottom: 1px dashed #dae9ee;
}
.intention-page .intention-summary .summary-box .wx-p {
  font-size: 13px;
  color: #666;
  line-height: 24px;
}
.intention-page .intention-flow {
  background: url(https://m.waitui.com/htdocs/mobile/images/intention-flow-bg.png) #fff no-repeat;
  background-position: center center;
  padding: 50px 0 50px 0;
}
.intention-page .intention-flow .flow-step {
  position: relative;
  width: 100%;
  height: 1000px;
  overflow: hidden;
  background: url(https://m.waitui.com/htdocs/mobile/images/intention-flow.png) no-repeat;
  background-position: top center;
}
.intention-page .intention-flow .flow-step .step-cell {
  position: absolute;
  display: table;
  width: 150px;
  height: 150px;
  background: url(https://m.waitui.com/htdocs/mobile/images/intention-flow-cell.png) no-repeat;
  background-position: center center;
  background-size: 100% 100%;
}
.intention-page .intention-flow .flow-step .step-cell .wx-p {
  display: table-cell;
  font-size: 16px;
  color: #999;
  text-align: center;
  vertical-align: middle;
  line-height: 24px;
}
.intention-page .intention-flow .flow-step .step-cell-1 {
  right: 50%;
  top: 36px;
  margin-right: 20px;
}
