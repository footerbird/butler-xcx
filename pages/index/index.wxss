/*变量*/
/*常用的一些混合*/
.ellip {
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.after-cls:after {
  content: " ";
  display: block;
  height: 0;
  visibility: hidden;
  clear: both;
}
.display-flex {
  display: -webkit-box;
  display: -moz-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
}
.forbid {
  opacity: 0.8 !important;
  cursor: not-allowed;
}
.entry-banner {
  display: block;
  width: 100%;
  height: 360rpx;
  background-color: #fff;
}
.entry-banner .swiper-slide {
  display: block;
  width: 100%;
  height: 100%;
}
.entry-banner .swiper-slide image {
  display: block;
  width: 100%;
  height: 100%;
}
.entry-subject {
  margin-top: 15px;
  background-color: #fff;
  padding: 0 15px;
}
.entry-subject .entry-subject-grids {
  display: -webkit-box;
  display: -moz-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  position: relative;
}
.entry-subject .entry-subject-grids .item {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
  flex: 1;
  position: relative;
  line-height: 70px;
  text-align: center;
  font-family: PingFang SC Bold;
  font-size: 14px;
  font-weight: bold;
  color: #333;
}
.entry-subject .entry-subject-grids .item.mid {
  -webkit-box-flex: 1.1;
  -webkit-flex: 1.1;
  flex: 1.1;
}
.entry-subject .entry-subject-grids .item.mid:before {
  content: '';
  position: absolute;
  left: 0;
  top: 50%;
  width: 1px;
  height: 30px;
  background-color: #f1f1f1;
  margin-top: -15px;
  -webkit-transform-origin: center center;
  transform-origin: center center;
  -webkit-transform: scaleY(0.5);
  transform: scaleY(0.5);
}
.entry-subject .entry-subject-grids .item.mid:after {
  content: '';
  position: absolute;
  right: 0;
  top: 50%;
  width: 1px;
  height: 30px;
  background-color: #f1f1f1;
  margin-top: -15px;
  -webkit-transform-origin: center center;
  transform-origin: center center;
  -webkit-transform: scaleY(0.5);
  transform: scaleY(0.5);
}
.entry-subject .entry-subject-grids .item .wx-i.user {
  display: inline-block;
  width: 18px;
  height: 18px;
  background: url(https://m.waitui.com/htdocs/mobile/images/entry/entry_subject_user.png) no-repeat;
  background-size: 100% 100%;
  vertical-align: -4px;
  margin-right: 8px;
}
.entry-subject .entry-subject-grids .item .wx-i.fire {
  display: inline-block;
  width: 18px;
  height: 18px;
  background: url(https://m.waitui.com/htdocs/mobile/images/entry/entry_subject_fire.png) no-repeat;
  background-size: 100% 100%;
  vertical-align: -4px;
  margin-right: 8px;
}
.entry-subject .entry-subject-grids .item .wx-i.crown {
  display: inline-block;
  width: 18px;
  height: 18px;
  background: url(https://m.waitui.com/htdocs/mobile/images/entry/entry_subject_crown.png) no-repeat;
  background-size: 100% 100%;
  vertical-align: -4px;
  margin-right: 8px;
}
.entry-subject .entry-subject-grids .item .wx-i.hot {
  position: absolute;
  left: 48px;
  top: 6px;
  width: 32px;
  height: 18px;
  background: url(https://m.waitui.com/htdocs/mobile/images/entry/entry_subject_hot.png) no-repeat;
  background-size: 100% 100%;
}
.entry-subject .entry-subject-grids:after {
  content: '';
  position: absolute;
  left: 0;
  bottom: 0;
  right: 0;
  border-bottom: 1px solid #f0f0f0;
  -webkit-transform-origin: 0 0;
  transform-origin: 0 0;
  -webkit-transform: scaleY(0.5);
  transform: scaleY(0.5);
}
.home-circle {
  margin-top: 15px;
  background-color: #fff;
}
.home-circle .title {
  font-size: 24px;
  font-weight: 400;
  height: 92px;
  line-height: 92px;
  text-align: center;
}
.home-circle .circle-tab {
  background-color: #f5f7fa;
  border-bottom: 1px solid #e5e5e5;
}
.home-circle .circle-tab:after {
  content: " ";
  display: block;
  height: 0;
  visibility: hidden;
  clear: both;
}
.home-circle .circle-tab .wx-li {
  float: left;
  width: 33.33%;
}
.home-circle .circle-tab .wx-li .wx-a {
  display: block;
  position: relative;
  text-align: center;
}
.home-circle .circle-tab .wx-li .wx-a .wx-em {
  display: block;
  font-size: 16px;
  font-weight: 400;
  line-height: 58px;
}
.home-circle .circle-tab .wx-li .wx-a .wx-em:hover {
  color: #00a4ff;
}
.home-circle .circle-tab .wx-li.cur .wx-a .wx-em {
  color: #00a4ff;
}
.home-circle .circle-tab .wx-li.cur .wx-a:before {
  content: '';
  position: absolute;
  z-index: 2;
  left: 50%;
  bottom: -4px;
  margin-left: -4px;
  width: 8px;
  height: 8px;
  background-color: #fff;
  border-top: 1px solid #00a4ff;
  border-left: 1px solid #00a4ff;
  -webkit-transform: rotate(45deg);
  transform: rotate(45deg);
}
.home-circle .circle-tab .wx-li.cur .wx-a:after {
  content: '';
  position: absolute;
  z-index: 1;
  left: 0;
  bottom: -1px;
  width: 100%;
  height: 1px;
  background-color: #00a4ff;
}
.home-circle .circle-content .circle-item .circle-service .service-block {
  display: block;
  width: 100%;
  height: 140px;
  padding: 20px 15px;
  border-bottom: 1px solid #e5e8ed;
  background-color: #fff;
  box-shadow: 0 2px 4px rgba(3, 27, 78, 0.06);
}
.home-circle .circle-content .circle-item .circle-service .service-block .service-block-title {
  position: relative;
  height: 28px;
}
.home-circle .circle-content .circle-item .circle-service .service-block .service-block-title .wx-em {
  display: inline;
  position: relative;
  font-size: 18px;
  color: #000;
  line-height: 28px;
}
.home-circle .circle-content .circle-item .circle-service .service-block .service-block-title .wx-em .hot {
  position: absolute;
  top: -10px;
  right: -18px;
  width: 18px;
  height: 18px;
  background: url(https://m.waitui.com/htdocs/mobile/images/entry/entry_subject_fire.png) no-repeat;
  background-size: 100% 100%;
}
.home-circle .circle-content .circle-item .circle-service .service-block .service-block-title .wx-span {
  position: absolute;
  right: 0;
  top: 3px;
}
.home-circle .circle-content .circle-item .circle-service .service-block .service-block-title .wx-span .wx-i {
  display: inline-block;
  font-size: 12px;
  color: #999;
  text-align: center;
  line-height: 20px;
  padding: 0 8px;
  border: 1px solid #e5e8ed;
  background-color: #fff;
}
.home-circle .circle-content .circle-item .circle-service .service-block .service-block-text {
  font-size: 13px;
  color: #666;
  line-height: 22px;
  margin-top: 4px;
}
.home-circle .circle-content .circle-item .circle-service .service-block:hover {
  -webkit-transition: all linear 0.2s;
  transition: all linear 0.2s;
  box-shadow: 0 4px 8px rgba(3, 27, 78, 0.12);
}
.home-circle .circle-content .circle-item .circle-service .service-block:hover .service-block-title .wx-em {
  -webkit-transition: all linear 0.2s;
  transition: all linear 0.2s;
  color: #00a4ff;
}
.home-solution {
  margin-top: 15px;
  background-color: #fff;
}
.home-solution .title {
  font-size: 24px;
  font-weight: 400;
  height: 92px;
  line-height: 92px;
  text-align: center;
}
.home-solution .solution-content {
  border-top: 1px solid #e5e8ed;
  background-color: #f5f7fa;
}
.home-solution .solution-content .solution-item .solution-service .service-block {
  display: block;
  position: relative;
  width: 100%;
  border-bottom: 1px solid #e5e8ed;
}
.home-solution .solution-content .solution-item .solution-service .service-block .service-block-title {
  position: relative;
  height: 50px;
  padding: 0 40px 0 15px;
  background-color: #fff;
}
.home-solution .solution-content .solution-item .solution-service .service-block .service-block-title .wx-em {
  font-size: 16px;
  color: #000;
  line-height: 50px;
}
.home-solution .solution-content .solution-item .solution-service .service-block .service-block-title:after {
  content: '';
  position: absolute;
  right: 13px;
  top: 50%;
  width: 18px;
  height: 18px;
  background: url(https://m.waitui.com/htdocs/mobile/images/home-solution-arrow.png) no-repeat;
  background-size: 36px 18px;
  background-position: 0 0;
  margin-top: -9px;
}
.home-solution .solution-content .solution-item .solution-service .service-block .service-block-text {
  display: none;
  font-size: 14px;
  color: #666;
  line-height: 24px;
  padding: 25px 15px 0 15px;
}
.home-solution .solution-content .solution-item .solution-service .service-block .service-block-price {
  display: none;
  font-size: 16px;
  color: #ff6600;
  line-height: 32px;
  padding: 0 15px 20px 15px;
}
.home-solution .solution-content .solution-item .solution-service .service-block .service-block-price .wx-i {
  font-size: 14px;
  margin-right: 2px;
}
.home-solution .solution-content .solution-item .solution-service .service-block .service-block-price .wx-a {
  float: right;
  position: relative;
  font-size: 14px;
  height: 32px;
  line-height: 32px;
  padding-right: 20px;
  color: #00a4ff;
}
.home-solution .solution-content .solution-item .solution-service .service-block .service-block-price .wx-a:after {
  content: '';
  position: absolute;
  right: 0;
  top: 50%;
  width: 10px;
  height: 10px;
  margin-top: -5px;
  background: url(https://m.waitui.com/htdocs/mobile/images/home-solution-price.png) no-repeat;
  background-size: 100% 100%;
}
.home-solution .solution-content .solution-item .solution-service .service-block:hover {
  -webkit-transition: all linear 0.2s;
  transition: all linear 0.2s;
  box-shadow: 0 4px 8px rgba(3, 27, 78, 0.12);
}
.home-solution .solution-content .solution-item .solution-service .service-block:hover .service-block-title .wx-em {
  -webkit-transition: all linear 0.2s;
  transition: all linear 0.2s;
  color: #00a4ff;
}
.home-solution .solution-content .solution-item .solution-service .service-block.cur {
  box-shadow: 0 4px 8px rgba(3, 27, 78, 0.12);
}
.home-solution .solution-content .solution-item .solution-service .service-block.cur .service-block-title .wx-em {
  color: #00a4ff;
}
.home-solution .solution-content .solution-item .solution-service .service-block.cur .service-block-title:after {
  background-position: -18px 0;
}
.home-solution .solution-content .solution-item .solution-service .service-block.cur .service-block-text {
  display: block;
}
.home-solution .solution-content .solution-item .solution-service .service-block.cur .service-block-price {
  display: block;
}
